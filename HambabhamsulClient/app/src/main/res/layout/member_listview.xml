<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearlayoutMemberProfile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/circleImageViewMemberProfile"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:layout_gravity="left"
        android:paddingTop="20dp"
        android:layout_weight="1"
        android:layout_margin="5dp"
        android:src="@drawable/hambabee" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        android:layout_weight="4"
        android:orientation="vertical">


        <TextView
            android:id="@+id/textViewMemberListViewManager"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_weight="3"
            android:text = "test"
            android:textColor="#000000"
            android:textSize="24sp"
            android:fontFamily="@font/nanumsquareaceb" />

        <TextView
            android:id="@+id/textViewMemberListViewPosition"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:text = "test"
            android:layout_weight="1"
            android:textSize="16sp"
            android:textColor="#000000"
            android:fontFamily="@font/nanumsquareacb" />
    </LinearLayout>


</LinearLayout>