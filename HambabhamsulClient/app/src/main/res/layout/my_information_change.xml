<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollViewChangeScroll"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageButton
            android:id="@+id/imageButtonChangeProfile"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/image_button"
            android:layout_centerHorizontal="true"
            android:scaleType="fitCenter"/>

        <TextView
            android:id="@+id/num2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageButtonChangeProfile"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="40dp"
            android:text="아이디"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <EditText
            android:id="@+id/editTextChangeId"
            android:layout_width="270dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/num2"
            android:layout_marginLeft="40dp"
            android:fontFamily="@font/dxmsubtitlesmkscpceuch" />

        <TextView
            android:id="@+id/num3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/editTextChangeId"
            android:layout_marginLeft="40dp"
            android:text="비밀번호"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <EditText
            android:id="@+id/editTextChangePassword"
            android:layout_width="270dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/num3"
            android:layout_marginLeft="40dp"
            android:password="true"
            android:fontFamily="@font/dxmsubtitlesmkscpceuch" />

        <TextView
            android:id="@+id/num5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/editTextChangePassword"
            android:layout_marginLeft="40dp"
            android:text="비밀번호 확인"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <ImageView
            android:id="@+id/unlocked"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignTop="@id/num5"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/num5"
            android:src="@drawable/unlocked"
            android:visibility="visible" />

        <ImageView
            android:id="@+id/locked"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignTop="@id/num5"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/num5"
            android:src="@drawable/locked"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/editTextChangePasswordCheck"
            android:layout_width="270dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/num5"
            android:layout_marginLeft="40dp"
            android:password="true"
            android:fontFamily="@font/dxmsubtitlesmkscpceuch" />

        <TextView
            android:id="@+id/num6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/editTextChangePasswordCheck"
            android:layout_marginLeft="40dp"
            android:text="이름"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <EditText
            android:id="@+id/editTextChangeName"
            android:layout_width="270dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/num6"
            android:layout_marginLeft="40dp"
            android:fontFamily="@font/dxmsubtitlesmkscpceuch" />

        <TextView
            android:id="@+id/num7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/editTextChangeName"
            android:layout_marginLeft="40dp"
            android:text="생년월일"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <EditText
            android:id="@+id/editTextChangeYear"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/num7"
            android:layout_marginLeft="40dp"
            android:hint="년(4자)"
            android:fontFamily="@font/dxmsubtitlesmkscpceuch" />

        <EditText
            android:id="@+id/editTextChangeMonth"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/editTextChangeYear"
            android:layout_marginLeft="135dp"
            android:hint="월"
            android:fontFamily="@font/dxmsubtitlesmkscpceuch" />

        <EditText
            android:id="@+id/editTextChangeDay"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/editTextChangeMonth"
            android:layout_marginLeft="230dp"
            android:hint="일"
            android:fontFamily="@font/dxmsubtitlesmkscpceuch" />

        <TextView
            android:id="@+id/num8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/editTextChangeYear"
            android:layout_marginLeft="40dp"
            android:text="성별"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <RadioGroup
            android:id="@+id/radioGroupChangeGender"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/editTextChangeYear"
            android:layout_toRightOf="@id/num8"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButtonChangeMale"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:text="남자"
                android:fontFamily="@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeFemale"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:text="여자"
                android:fontFamily="@font/nanumsquareacb" />
        </RadioGroup>

        <TextView
            android:id="@+id/num9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/num8"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="12dp"
            android:text="닉네임"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <EditText
            android:id="@+id/editTextChangeNickname"
            android:layout_width="270dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/num9"
            android:layout_marginLeft="40dp"
            android:fontFamily="@font/dxmsubtitlesmkscpceuch" />

        <TextView
            android:id="@+id/num10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/editTextChangeNickname"
            android:layout_marginLeft="40dp"
            android:text="나의 성격"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <RadioGroup
            android:id="@+id/radioGroupChangeCharacter1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/num10"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButtonChangeChar1_1"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="10dp"
                android:text="외향적인"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeChar1_2"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="10dp"
                android:text="내향적인"
                android:fontFamily = "@font/nanumsquareacb" />
        </RadioGroup>

        <RadioGroup
            android:id="@+id/radioGroupChangeCharacter2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/radioGroupChangeCharacter1"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButtonChangeChar2_1"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:text="감성적인"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeChar2_2"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:text="이성적인"
                android:fontFamily = "@font/nanumsquareacb" />
        </RadioGroup>

        <RadioGroup
            android:id="@+id/radioGroupChangeCharacter3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/radioGroupChangeCharacter2"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButtonChangeChar3_1"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:text="평범한"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeChar3_2"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:text="개성있는"
                android:fontFamily = "@font/nanumsquareacb" />
        </RadioGroup>

        <RadioGroup
            android:id="@+id/radioGroupChangeCharacter4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/radioGroupChangeCharacter3"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButtonChangeChar4_1"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:text="장난스러운"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeChar4_2"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:text="진중한"
                android:fontFamily = "@font/nanumsquareacb" />
        </RadioGroup>

        <RadioGroup
            android:id="@+id/radioGroupChangeCharacter5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/radioGroupChangeCharacter4"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButtonChangeChar5_1"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:text="성급한"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeChar5_2"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:text="여유로운"
                android:fontFamily = "@font/nanumsquareacb" />
        </RadioGroup>

        <TextView
            android:id="@+id/num11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/radioGroupChangeCharacter5"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:text="밥먹는 양"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <RadioGroup
            android:id="@+id/radioGroupChangeEatingAmount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/num11">

            <RadioButton
                android:id="@+id/radioButtonChangeLittle"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:text="1인분도\n많아요!"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeProper"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="1인분이\n적당해요!"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeMany"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="1인분은\n부족하죠!"
                android:fontFamily = "@font/nanumsquareacb" />
        </RadioGroup>

        <TextView
            android:id="@+id/num12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/radioGroupChangeEatingAmount"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:text="밥먹는 속도"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <RadioGroup
            android:id="@+id/radioGroupChangeEatingSpeed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/num12">

            <RadioButton
                android:id="@+id/radioButtonChangeSlow"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:text="느려요!"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeMiddle"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="적당해요!"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonChangeFast"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="빨라요!"
                android:fontFamily = "@font/nanumsquareacb" />
        </RadioGroup>

        <TextView
            android:id="@+id/textViewAnomity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/radioGroupChangeEatingSpeed"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:text="개인정보 공개 여부"
            android:layout_marginBottom="20dp"
            android:textSize="12pt"
            android:fontFamily = "@font/nanumsquareacb" />

        <RadioGroup
            android:id="@+id/radioGroupAnomityCheck"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="40dp"
            android:layout_marginBottom="20dp"
            android:layout_below="@+id/textViewAnomity">

            <RadioButton
                android:id="@+id/radioButtonAnomityTrue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="공개"
                android:fontFamily = "@font/nanumsquareacb" />

            <RadioButton
                android:id="@+id/radioButtonAnomityFalse"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="비공개"
                android:fontFamily = "@font/nanumsquareacb" />

        </RadioGroup>
    </RelativeLayout>
</ScrollView>